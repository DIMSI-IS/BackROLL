{
  "realm": "backroll",
  "enabled": true,
  "users": [
    {
      "username": "$sso_user_name",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "$sso_user_password"
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "backroll-front",
      "enabled": true,
      "publicClient": true,
      "baseUrl": "$front_url/admin/dashboard",
      "redirectUris": [
        "$front_url/*"
      ],
      "webOrigins": [
        "*"
      ]
    },
    {
      "clientId": "backroll-api",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "$sso_client_secret",
      "redirectUris": [
        "http://$api_address:5050/*"
      ],
      "webOrigins": [
        "*"
      ],
      "serviceAccountsEnabled": true
    }
  ]
}